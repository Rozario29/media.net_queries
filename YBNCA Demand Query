-- 		SUM(advertiser_value) ,
-- 		SUM(audited_revenue) 
--       FROM joining_hb_tac_data_with_meta_data


----------------------------------------------------------------------------------------------------------------------------

--TAC DISTRIBUTION

,tac_distribution
( 
SELECT 
  *,
  ((adclicks)/(customer_total_adclicks)) * (hb_tac) as hb_tac_dist
FROM	 	
  joining_hb_tac_data_with_meta_data
)

--SELECT SUM(net_total_revenue),
