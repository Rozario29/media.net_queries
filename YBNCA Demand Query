  b.integration_type_name,
b.integration_type_id
FROM
    final_23 a
LEFT JOIN misc.vogon_apr_23_cust_itype_mapping b
  ON
 	a.hb_customer_name = b.hb_customer_name
 	AND a.hb_customer_id = b.hb_customer_id
)


