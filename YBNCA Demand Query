--Adding MAX & NON MAX flag

,ybnca_data_from_vogon_with_max_flag

(
	select 
  			*,
			case when provider_group_id in (12,61) then 'MAX' else 'NON_MAX' end as max_flag
	from 
  			misc.april_22_ybnca_data_from_vogon
)


